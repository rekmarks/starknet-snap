{
  "name": "@metamask/starkware-snap",
  "version": "0.7.0",
  "private": true,
  "description": "The 'Hello, world!' of Starkware Snaps.",
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/snap-template.git"
  },
  "license": "MIT",
  "main": "src/index.js",
  "files": [
    "dist/",
    "images/",
    "snap.manifest.json"
  ],
  "scripts": {
    "build:clean": "yarn clean && yarn build",
    "build:website": "node ./scripts/build-website.js",
    "build:old": "mm-snap build --stripComments --manifest false --writeManifest false --eval false && node ./post-process.js && mm-snap manifest --fix && mm-snap eval",
    "build": "mm-snap build --stripComments",
    "serve": "mm-snap serve",
    "clean": "rimraf dist/*",
    "test": "echo 'TODO'",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' --ignore-path .gitignore",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "lint:changelog": "yarn auto-changelog validate"
  },
  "dependencies": {
    "@metamask/key-tree": "^3.0.1",
    "starknet": "^2.5.1"
  },
  "devDependencies": {
    "@metamask/auto-changelog": "^2.5.0",
    "@metamask/eslint-config": "^8.0.0",
    "@metamask/eslint-config-jest": "^8.0.0",
    "@metamask/eslint-config-nodejs": "^8.0.0",
    "@metamask/snaps-cli": "^0.9.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "mkdirp": "^1.0.4",
    "patch-package": "^6.4.7",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=12.11.0"
  }
}
